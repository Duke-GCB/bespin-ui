<span class="question-name">{{question.name}}</span>:
{{#if question.isFile}}
  {{questionnaire.file-answers questionProxy}}
{{/if}}
{{#if question.isString}}
  {{questionnaire.string-answers questionProxy}}
{{/if}}
{{#if question.isInteger}}
  {{questionnaire.integer-answers questionProxy}}
{{/if}}
{{#if question.isDirectory}}
  {{questionnaire.directory-answers questionProxy}}
{{/if}}
{{yield}}
