{{#if hasReadOnlyAnswers}}
  {{questionnaire.read-only-answers readOnlyAnswers label=question.name}}
{{else}}
  {{#if question.isFile}}
    {{questionnaire.dds-file-answer question.occurs (action 'answered') label=question.name}}
  {{/if}}
  {{#if question.isString}}
    {{questionnaire.string-answer ""}}
  {{/if}}
{{/if}}
{{yield}}
