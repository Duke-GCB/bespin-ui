{{#bs-form as |form|}}
  {{yield}}
  {{#form.group class='select-project'}}
    {{dds.dds-project-picker projects project (action 'projectChanged')}}
  {{/form.group}}
  {{#form.group class='select-files'}}
    {{dds.dds-file-picker project selectedResources (action 'filePicked')}}
  {{/form.group}}
{{/bs-form}}
